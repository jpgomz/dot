#!/bin/sh

echo "Replacing ~/.config/typecraft with symlink/copy."
ln -sf "$PWD/typecraft" "$HOME/.config/typecraft"

echo "Replacing ~/.config/LazyVim with symlink/copy."
ln -sf "$PWD/LazyVim" "$HOME/.config/LazyVim"

echo "Replacing ~/.config/LazyMisha with symlink/copy."
ln -sf "$PWD/LazyMisha" "$HOME/.config/LazyMisha"

echo "Replacing ~/.config/ThePrimeagen with symlink/copy."
ln -sf "$PWD/ThePrimeagen/init.vim" "$HOME/.config/nvim/init.vim"
